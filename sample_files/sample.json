      "pagination": {
        "page": 1,
        "perPage": 10,
        "total": 100,
        "totalPages": 10
      }
    },
    "meta": {
      "requestId": "abc123def456",
      "timestamp": "2024-01-15T12:00:00.000Z",
      "duration": 45
    }
  },

  "geojson": {
    "type": "FeatureCollection",
    "features": [
      {
        "type": "Feature",
        "geometry": {
          "type": "Point",
          "coordinates": [-122.4194, 37.7749]
        },
        "properties": {
          "name": "San Francisco",
          "population": 884363
        }
      },
      {
        "type": "Feature",
        "geometry": {
          "type": "Polygon",
          "coordinates": [
            [
              [-122.5, 37.7],
              [-122.3, 37.7],
              [-122.3, 37.9],
              [-122.5, 37.9],
              [-122.5, 37.7]
            ]
          ]
        },
        "properties": {
          "name": "Area",
          "type": "boundary"
        }
      }
    ]
  },

  "jsonSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "age": {
        "type": "integer",
        "minimum": 0,
        "maximum": 150
      },
      "email": {
        "type": "string",
        "format": "email"
      },
      "tags": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "uniqueItems": true
      }
    },
    "required": ["name", "email"],
    "additionalProperties": false
  },

  "i18n": {
    "en": {
      "greeting": "Hello",
      "farewell": "Goodbye",
      "welcome": "Welcome, {{name}}!"
    },
    "de": {
      "greeting": "Hallo",
      "farewell": "Auf Wiedersehen",
      "welcome": "Willkommen, {{name}}!"
    },
    "ja": {
      "greeting": "こんにちは",
      "farewell": "さようなら",
      "welcome": "ようこそ、{{name}}さん!"
    }
  },

  "veryLongString": "Lorem ipsum dolor sit amet, consectetur adipiscing elit. Sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat.",

  "largeNumber": 9007199254740991,
  "preciseFloat": 0.1234567890123456789,

  "specialCharacters": {
    "quotes": "He said, \"Hello!\"",
    "backslash": "C:\\Users\\Name\\Documents",
    "forwardSlash": "/usr/local/bin",
    "tabs": "Column1\tColumn2\tColumn3",
    "newlines": "Line1\nLine2\nLine3",
    "carriageReturn": "Line1\r\nLine2",
    "formFeed": "Page1\fPage2",
    "backspace": "Back\bspace",
    "unicodeEscapes": "\u0048\u0065\u006C\u006C\u006F"
  }
