    {
      "id": 2,
      "name": "Item 2",
      "active": false
    },
    {
      "id": 3,
      "name": "Item 3",
      "active": true
    }
  ],

  "complexStructure": {
    "metadata": {
      "version": "1.0.0",
      "author": "Sample Author",
      "created": "2024-01-15T12:00:00Z",
      "tags": ["sample", "json", "syntax"]
    },
    "configuration": {
      "debug": true,
      "maxRetries": 3,
      "timeout": 5000,
      "endpoints": {
        "api": "https://api.example.com",
        "auth": "https://auth.example.com",
        "cdn": "https://cdn.example.com"
      }
    },
    "features": [
      {
        "name": "feature1",
        "enabled": true,
        "config": {
          "option1": "value1",
          "option2": 100
        }
      },
      {
        "name": "feature2",
        "enabled": false,
        "config": null
      }
    ]
  },

  "database": {
    "host": "localhost",
    "port": 5432,
    "name": "myapp_production",
    "credentials": {
      "username": "admin",
      "password": "********"
    },
    "pool": {
      "min": 5,
      "max": 20,
      "idleTimeout": 30000
    },
    "ssl": {
      "enabled": true,
      "rejectUnauthorized": true,
      "ca": "/path/to/ca.pem"
    }
  },

  "users": [
    {
      "id": 1,
      "username": "alice",
      "email": "alice@example.com",
      "roles": ["admin", "user"],
      "profile": {
        "firstName": "Alice",
        "lastName": "Johnson",
        "avatar": "https://example.com/avatars/alice.png",
        "bio": "Software developer and open source enthusiast."
      },
      "settings": {
        "theme": "dark",
        "language": "en",
        "notifications": {
          "email": true,
          "push": false,
          "sms": false
        }
