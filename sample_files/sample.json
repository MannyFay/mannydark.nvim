{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/sample.json",

  "string": "Hello, JSON!",
  "stringWithEscapes": "Line 1\nLine 2\tTabbed",
  "stringWithUnicode": "Hello, 世界! \u0048\u0065\u006C\u006C\u006F",
  "emptyString": "",

  "integer": 42,
  "negativeInteger": -17,
  "float": 3.14159,
